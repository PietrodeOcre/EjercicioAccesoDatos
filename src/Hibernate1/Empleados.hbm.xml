<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="Empleado" table="Empleados" >
    <id column="emp_no" name="emp_no" type="integer"/>
    <property column="apellido" name="apellido" type="string"/>
    <property column="oficio" name="oficio" type="string"/>
    <property column="dir" name="dir" type="string"/>
    <property column="fecha_atl" name="fecha_atl" type="date"/>
    <property column="salario" name="salario" type="float"/>
    <property column="comision" name="comision" type="float"/>
    <property column="dep_no" name="dep_no" type="integer"/>
    
  </class>
</hibernate-mapping>